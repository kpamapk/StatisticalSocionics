---
import { getCollection } from "astro:content";
import Layout from "@/layouts/Layout.astro";

const entries = await getCollection("neurosocionics");
---

<Layout title="Neurosocionics" description="Brain and personality in Socionics.">
  <h1>Neurosocionics</h1>
  <ul>
    {entries.map((entry) => (
      <li><a href={`/neurosocionics/${entry.id}`}>{entry.data.title}</a></li>
    ))}
  </ul>
</Layout>
